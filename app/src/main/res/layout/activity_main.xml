<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context="com.example.windows.intent.MainActivity">
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="限时启动Activity"
    android:onClick="componentname"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐式启动Activity"
        android:onClick="intentfilter"/>
<EditText
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="http:?///www.gdmec.cn"
    android:id="@+id/url" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:text="打开网页"
        android:onClick="view"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:text="110"
        android:id="@+id/phone"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拨号"
        android:onClick="dial"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转换"
        android:onClick="startactivityforresult"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview1" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview1"
        android:text="等待转换"/>
</RelativeLayout>
